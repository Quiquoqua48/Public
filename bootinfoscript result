	insmod part_gpt
	insmod ext2
	set root='hd0,gpt2'
	if [ x$feature_platform_search_hint = xy ]; then
	  search --no-floppy --fs-uuid --set=root --hint-bios=hd0,gpt2 --hint-efi=hd0,gpt2 --hint-baremetal=ahci0,gpt2  0643c84d-8a1d-41ca-a6c4-25167d48b554
	else
	  search --no-floppy --fs-uuid --set=root 0643c84d-8a1d-41ca-a6c4-25167d48b554
	fi
	linux /boot/vmlinuz-5.3.0-26-generic root=UUID=0643c84d-8a1d-41ca-a6c4-25167d48b554 ro quiet splash $vt_handoff
	initrd /boot/initrd.img-5.3.0-26-generic
}
