	  search --no-floppy --fs-uuid --set=root --hint-bios=hd0,gpt1 --hint-efi=hd0,gpt1 --hint-baremetal=ahci0,gpt1  245E-C65E
	else
	  search --no-floppy --fs-uuid --set=root 245E-C65E
	fi
	chainloader /EFI/Microsoft/Boot/bootmgfw.efi
}
